{
	"06-02-1933":{
		"character": "Frank",
		"start": {
			"text": "We need to talk Ben.",
			"options": [{
				"text": "Sure, tell me.",
				"then": "A"
			}]
		},
		"A": {
			"text": "The business is not doing so well, I'm afraid if we don't increase the sales, we should close.",
			"options": [{
				"text": "That doesn't sound good. Give us some time, Jhonatan is working on a big story",
				"then": "B"
			}]
		},
		"B": {
			"text": "A new story huh? We need something big like in your old good days.",
			"options": [{
				"text": "I'm still in the good days my friend."
			}]
		}
	},
	"13-02-1933":{
		"character": "Jhon",
		"start": {
			"text": "What are you going to do with the money?",
			"options": [{
				"text": "I'll put it back in one of that restaurants.",
				"then": "A"
				}, {
				"text": "We are running out of money, we can take some dollars to save the newspaper.",
				"then": "B"
			}]
		},
		"A": {
			"text": "Let Betty do that for you, she will not draw suspicion.",
			"options": [{
				"text": "You're right, it's better to focus on tomorrow's frontpage."
			}]
		},
		"B": {
			"text": "Are you kidding, right?",
			"options": [{
				"text": "Hahaha, obviously. Betty can return the money to Mr Montana."
			}]
		}
	},
	"16-02-1933":{
		"character": "Charlie",
		"start": {
			"text": "Mr Brown, its a shame that you regret our gift and also you publish that ugly things about our business.",
			"options": [{
				"text": "We are not afraid of you",
				"then": "B"
			}]
		},
		"B": {
			"text": "You should..., for your own good, moderate your language.",
			"options": [{
				"text": "Bye",
				"then": "C"
			}]
		},
		"C": {
			"character": "Jhonatan",
			"text": "what do you need?",
			"options": [{
				"text": "We don't have enough news ready to publish today, do you have something about Mr Montana?",
				"then": "D"
			}]
		},
		"D": {
			"text": "Yes, I'll finish a note for you right now.",
			"options": [{
				"text": "Thanks, the murder of Mr Roosvelt and this Montana thing...",
				"then": "E"
			}]
		},
		"E": {
			"text": "That's true, we haven't these \"happy days\" back.",
			"options": [{
				"text": "No, unfortunately..."
			}]
		}
	},
